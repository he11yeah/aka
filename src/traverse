# Change Directory
alias ..='cd ..'
alias ...='cd ../../../'
alias 4..='cd ../../../../'
alias 5..='cd ../../../../'

# Listing
alias l='ls -lFh'
alias la='ls -lAFh'
alias lr='ls -tRFh'
alias lt='ls -ltFh'
alias ll='ls -l'
alias lS='ls -1FSsh'
alias lrt='ls -1Fcrt'
alias ldot='ls -ld .*'
alias lart='ls -1Fcart'

# Others
alias fd='find . -type d -name'
alias ff='find . -type f -name'
alias h='history'
alias c='clear'
